{
  "env": {
    "development": {
      "presets": ["next/babel"],
      "plugins": [
        ["emotion", { "sourceMap": true, "autoLabel": true }],
        ["inline-import", {"extensions": [".css"]}]
      ]
    },
    "production": {
      "presets": ["next/babel"],
      "plugins": [
        ["emotion", { "hoist": true }],
        ["inline-import", {"extensions": [".css"]}]
      ]
    }
  },
  "plugins": ["transform-class-properties"]
}
