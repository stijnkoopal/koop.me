{
  "env": {
    "development": {
      "presets": ["next/babel"],
      "plugins": [
        ["emotion", { "sourceMap": true, "autoLabel": true }]
      ]
    },
    "production": {
      "presets": ["next/babel"],
      "plugins": [
        ["emotion", { "hoist": true }]
      ]
    },
    "test": {
      "presets": [["next/babel", { "preset-env": { "modules": "commonjs" } }]]
    }
  }
}
