{
  "env": {
    "development": {
      "presets": ["next/babel"],
      "plugins": [
        ["emotion", { "sourceMap": true, "autoLabel": true }],
        ["inline-import", {"extensions": [".css"]}]
      ]
    },
    "production": {
      "presets": ["next/babel"],
      "plugins": [
        ["emotion", { "hoist": true }],
        ["inline-import", {"extensions": [".css"]}]
      ]
    },
    "test": {
      "presets": [["next/babel", { "preset-env": { "modules": "commonjs" } }]]
    }
  },
  "plugins": ["transform-class-properties"]
}
