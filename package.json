  "jest": {},
  "scripts": {
    "build": "yarn build:generate-resume-pdf && yarn build:app && yarn build:lambda",
    "build:app": "NODE_ENV=production next build && next export",
    "build:lambda": "NODE_ENV=production netlify-lambda build lambda-src",
    "build:generate-resume-pdf": "resume export ./static/resume-stijn-koopal.pdf --force --format pdf --resume=./static/resume.json",
    "build:analyze": "BUNDLE_ANALYZE=browser yarn build",
    "dependencies": "ncu",
    "dependencies:upgrade": "ncu -ua",
    "dev": "concurrently \"yarn dev:app\" \"yarn dev:lambda\"",
    "dev:app": "NODE_ENV=development node server.js",
    "dev:lambda": "NODE_ENV=development netlify-lambda serve lambda-src",
    "format": "prettier --write *.js **/*.js **/*.jsx",
    "storybook": "start-storybook -p 6006",
    "test": "NODE_ENV=test jest --coverage"
  }
